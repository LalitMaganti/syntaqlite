# SQLite tokenizer extracted from SQLite source code.
# Regenerate with: python3 python/tools/extract_tokenizer.py

source_set("tokenizer") {
  sources = [
    # Extracted/generated from SQLite source
    "sqlite_tables.h",
    "sqlite_tokenize.c",

    # All the keywords and tokens but can be overwritten via injected code.
    "sqlite_keywordhash_data.h",
    "sqlite_tokens.h",

    # Manually maintained.
    "syntaqlite_defs.h",
  ]
}
